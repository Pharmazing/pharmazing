{
  "name": "pharmazing",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:clr": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "postinstall": "node ./scripts/postinstall.js",
    "web": "expo start --web",
    "lint": "eslint .",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "build:ios": "eas build -p ios --profile preview",
    "build:android": "eas build -p android --profile preview",
    "debug:SHA1": "cd android && keytool -keystore app/debug.keystore -list -v",
    "release:SHA1": "cd android && keytool -keystore app/release.keystore -list -v",
    "codegen": "graphql-codegen --config graphql.config.yml",
    "pod:install": "cd ios && pod install"
  },
  "dependencies": {
