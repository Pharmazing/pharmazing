{
  "name": "pharmazing",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:clr": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "build:ios": "eas build -p ios --profile preview",
    "build:android": "eas build -p android --profile preview",
    "debug:SHA1": "cd android && keytool -keystore app/debug.keystore -list -v",
    "release:SHA1": "cd android && keytool -keystore app/release.keystore -list -v",
    "codegen": "graphql-codegen --config graphql.config.yml",
    "pod:install": "cd ios && pod install"
  },
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@expo/metro-runtime": "~3.2.1",
    "@graphql-codegen/typescript-operations": "^4.2.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@react-native-google-signin/google-signin": "12.2.1",
    "@react-navigation/drawer": "^6.6.15",
    "@tamagui/config": "^1.101.5",
    "@tanstack/react-query": "^5.40.0",
    "expo": "~51.0.8",
    "expo-constants": "~16.0.1",
    "expo-dev-client": "^4.0.18",
    "expo-font": "^12.0.7",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.14",
    "expo-secure-store": "^13.0.1",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.6",
    "graphql": "^16.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.3.0",
    "react-native-web": "~0.19.10",
    "tamagui": "^1.101.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.0.7",
    "@parcel/watcher": "^2.4.1",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "private": true
}
