    "expo-system-ui": "~3.0.6",
    "geolib": "^3.3.4",
    "graphql": "^16.8.1",
    "native-base": "^3.4.28",
    "normalize-css-color": "^1.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
